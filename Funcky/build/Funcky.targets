<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <ItemGroup Condition="'$(FunckyImplicitUsings)' == 'true' Or '$(FunckyImplicitUsings)' == 'enable'">
        <Using Include="Funcky" />
        <Using Include="Funcky.Extensions" />
        <Using Include="Funcky.Monads" />
        <Using Include="Funcky.Functional" Static="true" />
    </ItemGroup>
	<Target	Name="_FunckyBeforePrepareForILLink" BeforeTargets="PrepareForILLink">
		<ItemGroup>
			<_ILLinkSuppressions Include="$(MSBuildThisFileDirectory)ILLink.LinkAttributes.xml" />
		</ItemGroup>
	</Target>
</Project>
